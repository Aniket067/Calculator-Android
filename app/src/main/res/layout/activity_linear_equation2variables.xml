<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Linear_equation2variables"
    android:background="@drawable/black_bg">
    <TextView
        android:id="@+id/home_screen3"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:background="@drawable/box_design"
        android:foreground="@drawable/ic_baseline_home_24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.048"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.04000002"/>
    <TextView
        android:id="@+id/retry3"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:background="@drawable/box_design"
        android:foreground="@drawable/ic_baseline_refresh_24"
        android:onClick="retry3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.96"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.04000002" />
    <TextView
        android:id="@+id/textView2000"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_marginTop="155dp"
        android:layout_marginBottom="557dp"
        android:gravity="center"
        android:text="Equation Is in form of ax+by+c=0 where x,y are variables"
        android:textColor="@color/white"
        android:textSize="28sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.53" />

    <TextView
        android:id="@+id/textView39"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:gravity="center|left"
        android:textColor="@color/white"
        android:textSize="20dp"
        android:layout_marginLeft="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2000"
        app:layout_constraintVertical_bias="0.035" />

    <LinearLayout
        android:id="@+id/linearLayout6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="0dp"
        android:layout_marginTop="398dp"
        android:foregroundGravity="bottom|center"
        android:gravity="bottom|center"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.76">

        <EditText
            android:id="@+id/editTextNumberSigned5"
            android:layout_width="160dp"
            android:layout_height="100dp"
            android:layout_gravity="bottom"
            android:layout_marginHorizontal="2dp"
            android:layout_weight="1"
            android:background="@drawable/box_design"
            android:baselineAligned="true"
            android:ems="10"
            android:foregroundGravity="bottom"
            android:gravity="center"
            android:hint="a2"
            android:inputType="numberSigned|numberDecimal"
            android:paddingLeft="7sp"
            android:textSize="30dp" />

        <EditText
            android:id="@+id/editTextNumberSigned6"
            android:layout_width="160dp"
            android:layout_height="100dp"
            android:layout_gravity="bottom"
            android:layout_marginHorizontal="2dp"
            android:layout_weight="1"
            android:background="@drawable/box_design"
            android:baselineAligned="false"
            android:ems="10"
            android:foregroundGravity="bottom"
            android:gravity="center"
            android:hint="b2"
            android:inputType="numberSigned|numberDecimal"
            android:paddingLeft="7sp"
            android:textSize="30dp" />

        <EditText
            android:id="@+id/editTextNumberSigned7"
            android:layout_width="160dp"
            android:layout_height="100dp"
            android:layout_marginHorizontal="2dp"
            android:layout_weight="1"
            android:background="@drawable/box_design"
            android:baselineAligned="false"
            android:ems="10"
            android:gravity="center"
            android:hint="c2"
            android:inputType="numberSigned|numberDecimal"
            android:paddingLeft="7sp"
            android:textSize="30dp" />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="0dp"
        android:layout_marginTop="398dp"
        android:foregroundGravity="bottom|center"
        android:gravity="bottom|center"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.58000004">

        <EditText
            android:id="@+id/editTextNumberSigned8"
            android:layout_width="160dp"
            android:layout_height="100dp"
            android:layout_gravity="bottom"
            android:layout_marginHorizontal="2dp"
            android:layout_weight="1"
            android:background="@drawable/box_design"
            android:baselineAligned="true"
            android:ems="10"
            android:foregroundGravity="bottom"
            android:gravity="center"
            android:hint="a1"
            android:inputType="numberSigned|numberDecimal"
            android:paddingLeft="7sp"
            android:textSize="30dp" />

        <EditText
            android:id="@+id/editTextNumberSigned9"
            android:layout_width="160dp"
            android:layout_height="100dp"
            android:layout_gravity="bottom"
            android:layout_marginHorizontal="2dp"
            android:layout_weight="1"
            android:background="@drawable/box_design"
            android:baselineAligned="false"
            android:ems="10"
            android:foregroundGravity="bottom"
            android:gravity="center"
            android:hint="b1"
            android:inputType="numberSigned|numberDecimal"
            android:paddingLeft="7sp"
            android:textSize="30dp" />

        <EditText
            android:id="@+id/editTextNumberSigned10"
            android:layout_width="160dp"
            android:layout_height="100dp"
            android:layout_marginHorizontal="2dp"
            android:layout_weight="1"
            android:background="@drawable/box_design"
            android:baselineAligned="false"
            android:ems="10"
            android:gravity="center"
            android:hint="c1"
            android:inputType="numberSigned|numberDecimal"
            android:paddingLeft="7sp"
            android:textSize="30dp" />
    </LinearLayout>
    <Button
        android:id="@+id/submitlinear2"
        android:layout_width="150dp"
        android:layout_height="61dp"
        android:layout_gravity="center"
        android:layout_marginHorizontal="2dp"
        android:background="@drawable/button_design2"
        android:foregroundGravity="center"
        android:gravity="center"
        android:shadowColor="@color/purple_500"
        android:text="@string/submit"
        android:textColor="@color/white"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/linearLayout5"
        app:layout_constraintVertical_bias="0.84000003" />

</androidx.constraintlayout.widget.ConstraintLayout>